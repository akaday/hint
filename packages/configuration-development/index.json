{
    "connector": "local",
    "extends": [
        "accessibility",
        "progressive-web-apps"
    ],
    "formatters": [
        "html",
        "summary"
    ],
    "hints": [
        "apple-touch-icons",
        "button-type",
        "compat-api/css",
        "compat-api/html",
        "create-element-svg",
        "css-prefix-order",
        "detect-css-reflows/composite",
        "detect-css-reflows/layout",
        "detect-css-reflows/paint",
        "disown-opener",
        "highest-available-document-mode",
        "leading-dot-classlist",
        "manifest-exists",
        "meta-charset-utf-8",
        "meta-viewport",
        "no-bom",
        "no-inline-styles",
        "no-protocol-relative-urls",
        "scoped-svg-styles",
        "sri",
        "typescript-config/consistent-casing",
        "typescript-config/is-valid",
        "typescript-config/strict",
        "typescript-config/target",
        "webpack-config/config-exists",
        "webpack-config/is-installed",
        "webpack-config/is-valid",
        "webpack-config/module-esnext-typescript",
        "webpack-config/modules-false-babel",
        "webpack-config/no-devtool-in-prod"
    ],
    "hintsTimeout": 10000,
    "parsers": [
        "babel-config",
        "css",
        "html",
        "javascript",
        "jsx",
        "less",
        "sass",
        "typescript",
        "typescript-config",
        "webpack-config"
    ]
}

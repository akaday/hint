{
    "connector": "puppeteer",
    "formatters": [
        "html",
        "summary"
    ],
    "hints": {
        "0": "x-content-type-options",
        "1": "webpack-config/config-exists",
        "2": "webpack-config/is-installed",
        "3": "webpack-config/is-valid",
        "4": "webpack-config/module-esnext-typescript",
        "5": "webpack-config/modules-false-babel",
        "6": "webpack-config/no-devtool-in-prod",
        "7": "axe/parsing",
        "8": "axe/semantics",
        "9": "axe/sensory-and-visual-cues",
        "10": "axe/structure",
        "11": "axe/tables",
        "12": "axe/text-alternatives",
        "13": "axe/time-and-media"
    },
    "hintsTimeout": 120000,
    "parsers": [
        "babel-config",
        "css",
        "html",
        "javascript",
        "jsx",
        "less",
        "manifest",
        "package-json",
        "sass",
        "typescript",
        "typescript-config",
        "webpack-config"
    ]
}
